{% for item in items %}
	<li>
		<a href="mailto:{{ item.content }}" title="Email" alt="Email">
			<span class="sr-only">Email</span>
			<i class="fas fa-envelope-square email-icon"></i>
		</a>
	</li>
{% endfor %}
